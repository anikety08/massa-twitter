{
  "name": "massa-social-stack",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "contracts",
    "web"
  ],
  "scripts": {
    "build": "npm run build -w contracts && npm run build -w web",
    "build:deweb": "npm run build -w contracts && npm run build:deweb -w web",
    "dev": "npm run dev -w web",
    "lint": "npm run lint -w web",
    "format": "npm run format -w web",
    "test": "npm run test -w web",
    "deploy:deweb": "npx @massalabs/deweb-cli upload web/out"
  },
  "author": "WaveHack",
  "license": "MIT",
  "description": "Massa WaveHack social dApp monorepo",
  "dependencies": {
    "@massalabs/as-types": "^2.1.0",
    "@massalabs/massa-web3": "^5.3.0"
  }
}
